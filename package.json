{
	"name": "haberdasher",
	"version": "2.0.0",
	"description": "A consistent-hash ring based on farmhash and redblack.js",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"clean": "rm -rf dist coverage .vitest",
		"lint": "eslint .",
		"lint-fix": "eslint . --fix",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"release": "standard-version"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arobson/haberdasher"
	},
	"keywords": [
		"consistent hash",
		"hash",
		"ring",
		"typescript",
		"esm"
	],
	"author": "Alex Robson",
	"license": "MIT License - http://opensource.org/licenses/MIT",
	"bugs": {
		"url": "https://github.com/arobson/haberdasher/issues"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"homepage": "https://github.com/arobson/haberdasher",
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@types/node": "^22.0.0",
		"@vitest/coverage-v8": "^2.0.0",
		"eslint": "^9.0.0",
		"standard-version": "^4.3.0",
		"typescript": "^5.6.0",
		"typescript-eslint": "^8.0.0",
		"vitest": "^2.0.0"
	},
	"dependencies": {
		"farmhash": "^3.3.1",
		"redblack.js": "^1.0.0"
	}
}
